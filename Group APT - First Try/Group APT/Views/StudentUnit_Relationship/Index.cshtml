                @Html.DisplayNameFor(model => model.StudentRelation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UnitRelation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
